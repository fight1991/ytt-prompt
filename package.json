{
  "name": "ytt-prompt",
  "version": "1.0.1-beta.1",
  "description": "",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "homepage": "https://github.com/fight1991/ytt-prompt.git",
  "scripts": {
    "createApi": "rimraf ytt/apis && node dist/cjs/core/cli.js",
    "build:publish": "npm run build:es && npm publish",
    "read": "node createEntry.js",
    "prompt": "node dist/lib/index.js",
    "build:lib": "rimraf dist/lib && rollup -c --bundleConfigAsCjs --environment BUILD:lib",
    "build:es": "rimraf dist/es && npm run read && rollup -c --bundleConfigAsCjs --environment BUILD:es",
    "build:cjs": "rimraf dist/cjs && rollup -c --bundleConfigAsCjs --environment BUILD:cjs"
  },
  "bin": {
    "prompt": "dist/lib/index.js"
  },
  "files": [
    "dist/es/*",
    "dist/lib/*"
  ],
  "author": "tiancong",
